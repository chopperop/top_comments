<div class="bodyPlacementMobile">	

<% if @site >= 31 %>
	<%= render 'redditMobile' %>
<% else %>
	<%= render 'imgurMobile' %>
<% end %>

<ul class="homeButtons">
	<li class="moveLeft"><%= link_to "Get Another Comment", root_path, class: "btn btn-primary" %></li>
	<li class="shareLeftFB"> 
		<div id="fb-root"></div>
		<div class="fb-like" data-href="https://www.facebook.com/stumweb" data-layout="button_count" data-action="like" 		 data-show-faces="false" data-share="false"></div>
	</li>
</ul>

</div>

